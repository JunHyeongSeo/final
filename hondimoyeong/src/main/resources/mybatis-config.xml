<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
  <configuration>

 	<settings>
 		<setting name="jdbcTypeForNull" value="NULL" />
 	</settings>
 	
 	<typeAliases>
 		<typeAlias type="com.kh.hondimoyeong.course.model.vo.Course" alias="course"/>
 		<typeAlias type="com.kh.hondimoyeong.course.model.vo.CourseFile" alias="courseFile" />
 		<typeAlias type="com.kh.hondimoyeong.cs.model.vo.Notice" alias="notice" />
 		<typeAlias type="com.kh.hondimoyeong.cs.model.vo.Question" alias="question" />
 		<typeAlias type="com.kh.hondimoyeong.cs.model.vo.Answer" alias="answer" />
 	</typeAliases>

	<mappers>
		<mapper resource="mapper/course-mapper.xml"/>
		<mapper resource="mapper/notice-mapper.xml"/>
	</mappers>
 	
 </configuration>